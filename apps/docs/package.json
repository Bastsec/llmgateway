{
	"name": "docs",
	"version": "0.0.0",
	"private": true,
	"files": [
		".source",
		".next",
		"out",
		"openapi.json"
	],
	"scripts": {
		"build": "fumadocs-mdx && tsc && next build && cp ../gateway/openapi.json .",
		"dev": "next dev --turbo --port 3005",
		"gen-docs": "node scripts/generate-docs.mjs",
		"generate": "pnpm gen-docs",
		"postinstall": "fumadocs-mdx || true",
		"start": "next start -H 0.0.0.0"
	},
	"dependencies": {
		"fumadocs-core": "15.6.9",
		"fumadocs-mdx": "11.7.4",
		"fumadocs-openapi": "9.1.10",
		"fumadocs-ui": "15.6.9",
		"gateway": "workspace:*",
		"next": "15.4.6",
		"posthog-js": "1.255.1",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"remark": "15.0.1",
		"remark-gfm": "4.0.1",
		"remark-mdx": "3.1.0",
		"shiki": "3.9.2"
	},
	"devDependencies": {
		"@tailwindcss/postcss": "^4.1.5",
		"@types/mdx": "^2.0.13",
		"@types/node": "24.0.3",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"eslint": "^9",
		"eslint-config-next": "15.4.1",
		"postcss": "^8.5.4",
		"rimraf": "6.0.1",
		"tailwindcss": "^4.1.5",
		"typescript": "^5.8.3"
	}
}
